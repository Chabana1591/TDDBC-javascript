{
  "extends": "standard",
  "globals": {
    "test": true,
    "expect": true
  },
  "rules": {
    "indent": "off",
    "quotes": "off",
    "semi": "off",
    "space-before-function-paren": "off",
    "comma-dangle": "off",
    "yoda":"off"
  },
  "env": {
    "jest/globals": true
  },
  "plugins": [
    "jest"
  ]
}
