{
  "extends": "standard",
  "globals": {
    "test": true,
    "expect": true
  },
  "rules": {
    "indent": "off",
    "quotes": "off",
    "semi": "off",
    "space-before-function-paren": "off",
    "comma-dangle": ["error", {
      "arrays": "never",
      "objects": "never",
      "imports": "never",
      "exports": "never",
      "functions": "never"
    }]
  },
  "env": {
    "jest/globals": true
  },
  "plugins": [
    "jest"
  ]
}
